
config samba 'def'
	option name 'b'
	option workgroup 'WORKGROUP'
	option description '路由器上的U盘'
	option homes '0'
	option autoshare '0'
	option enabled '1'

config sambashare 'sh1'
	option browseable 'yes'
	option name '70'
	option path '/mnt/sda1'
	option users 'root'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0666'
	option dir_mask '0777'

config sambashare 'sh2'
	option browseable 'yes'
	option name '电脑资料(只读)'
	option path '/mnt/sda1/000000'
	option read_only 'yes'
	option guest_ok 'yes'

config sambashare 'sh3'
	option browseable 'yes'
	option name '临时可写目录'
	option path '/mnt/sda1/temp'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0666'
	option dir_mask '0777'

